/* Album Comments Schemas */
{
  "comments": {
    "type": "ObjectId",
    "required": true,
    "ref": "Album"
  },
  "parent": {
    "type": "String",
    "required": true,
    "unique": true
  },
  "writer": {
    "type": "String",
    "required": true
  },
  "createAt": {
    "type": "Date",
    "default": "Date.now"
  },
  "modifyAt": {
    "type": "Date",
    "default": "Date.now"
  }
}
